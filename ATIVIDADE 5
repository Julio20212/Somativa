package AVSomativa;

import java.util.Scanner;

public class Ativsomativa5 {//Nome da classe

	public static void main(String[] args) {//Permite que o código seja executado de diversas formas
	Scanner ler = new Scanner(System.in);//Scanner lê e armazena
	
	String c[] = new String [6];//Declarando a variável c e repetição 6
	
	for ( int i = 0; i<6; i++) {//Estrutura de repetição
	System.out.println("Informe carro: ");//Usuário irá informar o carro
	c[i] = ler.next();//Será guardado na variável c
	}	
	for ( int i = 0; i<6; i++) {//Estrutura de repetição 
		System.out.println(c[i]);//Informar o resultado 
		}
	}
}
